<VirtualHost *:{{ https_port }}>
  ServerName www.example.org
  SSLEngine on
  SSLProxyEngine on
  SSLProtocol ALL -SSLv2 -SSLv3
  SSLCipherSuite HIGH:MEDIUM:!aNULL:!MD5:!RC4

  # Rewrite part
  ProxyVia On
  ProxyPreserveHost On
  ProxyTimeout 600
  RewriteEngine On


  ErrorDocument 404 /notfound.html

</VirtualHost>

<VirtualHost *:{{ http_port }}>

  ServerName www.example.org
  ErrorDocument 404 /notfound.html
</VirtualHost>