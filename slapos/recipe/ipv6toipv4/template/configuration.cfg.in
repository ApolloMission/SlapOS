global
  maxconn 4096

defaults
    log                 global
    mode                tcp
    option              dontlognull
    retries             3
    contimeout          5000
    clitimeout          50000
    srvtimeout          50000

listen  ipv6toipv4     %(ipv6)s:%(ipv6_port)s
        mode    tcp
        server  ipv4server    %(ipv4)s:%(ipv4_port)s
